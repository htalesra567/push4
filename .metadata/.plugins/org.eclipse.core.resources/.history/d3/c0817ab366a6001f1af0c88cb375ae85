package com.cdac.test;

public class RemoveDupli {
	
	public static void main(String[] args) {
		int[] arr = {1,1,2,4,4,5,5,6};
		int n = arr.length;
		int key = 0;
		for(int i=1;i<n;i++) {
			if(arr[key] != arr[i]) {
				arr[key++] = arr[i]; 
			}
		}
		for(int i=0;i<key;i++) {
			System.out.print(arr[i]);
		}
	}
}
